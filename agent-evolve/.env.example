# AgentEvolve Configuration
# Copy this file to .env and fill in your API keys

# Anthropic API Key (for Claude - recommended)
ANTHROPIC_API_KEY=sk-ant-your-key-here

# OpenAI API Key (for GPT-4 - optional)
OPENAI_API_KEY=sk-your-key-here

# Which LLM to use for the configuration agent
# Options: "anthropic" or "openai"
DEFAULT_LLM_CLIENT=anthropic

# Gradio UI Configuration (optional)
GRADIO_SERVER_PORT=7860
GRADIO_SHARE=false
